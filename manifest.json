{
  "manifest_version": 2,
  "name": "Hello World",
  "description": "Hello World!",
  "version": "0.0.1",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/workForAll.js"],
      "css": ["css/workForAll.css"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "popup/defaultPopUp.html",
    "default_title": "Pop up title"
  }
}